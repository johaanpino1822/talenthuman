{
    "headers": [
      {
        "source": "/documents/(.*)",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/pdf"
          },
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/documents/(.*)",
        "destination": "/documents/$1"
      }
    ]
  }